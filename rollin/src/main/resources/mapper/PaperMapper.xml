<?xml version="1.0" encoding="utf-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.poscoict.rollin.paper.repo.PaperMapper">
    <select id="getAllPaper" resultType="PaperDTO">
        SELECT userId,
               nickname,
               content, date, giftId
        FROM
            papers;
    </select>
    <insert id="postPaper" parameterType="PaperDTO">
        Insert Into
        papers(userId, nickname, content)
        VALUES
        (#{userId},#{nickname},#{content})
    </insert>

</mapper>